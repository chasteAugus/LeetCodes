//104.二叉树的最大深度
struct TreeNode {
    int val;
    TreeNode *left;
    TreeNode *right;
    TreeNode(int x) : val(x), left(NULL), right(NULL) {}
};

class Solution {//递归求解
public:
    int maxDepth(TreeNode* root) {
        if(root==NULL)
            return 0;
        int deepOfleft=maxDepth(root->left);
        int deepOfright=maxDepth(root->right);
        return max(deepOfleft,deepOfright)+1;
    }
};
